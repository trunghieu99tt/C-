bool launchSequenceChecker(std::vector<std::string> systemNames, std::vector<int> stepNumbers) {
    map<string,int> temp;
    for (int i=0;i<systemNames.size();i++)
    {
        if(temp.find(systemNames[i])!=temp.end())
        {
            if(stepNumbers[i]<=temp[systemNames[i]]) return 0;
            else temp[systemNames[i]]=stepNumbers[i];
        }
        else temp.insert(make_pair(systemNames[i],stepNumbers[i]));
    }
    return 1;
}
