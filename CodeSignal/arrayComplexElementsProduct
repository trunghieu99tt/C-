std::vector<int> arrayComplexElementsProduct(std::vector<int> real, std::vector<int> imag) {
    vector<int> ans{1, 0};
    for(int i=0; i<real.size(); ++i) {
        vector<int> tmp = ans;
        ans = {tmp[0]*real[i]-tmp[1]*imag[i], tmp[0]*imag[i]+tmp[1]*real[i]};
        
    }
    
    return ans;
}
