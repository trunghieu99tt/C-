std::vector<int> directionOfReading(std::vector<int> numbers) {
    vector<string> temp;
    vector<int>result;
    int i,j,k,n=numbers.size();
    for (i=0;i<n;i++)
    {
        string s=to_string(numbers[i]);
        while(s.length()!=n) s='0'+s;
        temp.push_back(s);
    }
    k=0;
    for (i=0;i<n;i++)
    {
        int res=0;
        for (j=0;j<n;j++)
            res=res*10+(temp[j][i]-'0');
        result.push_back(res);
    }
    return result;
}
