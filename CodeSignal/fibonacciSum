std::vector<int> fibonacciSum(int n) {
    vector<int> ans;
    vector<int> fib;
    fib.push_back(1);
    fib.push_back(1);
    while (fib[fib.size() - 1] + fib[fib.size() - 2] <= n)
        fib.push_back(fib[fib.size() - 1] + fib[fib.size() - 2]);
    int j = fib.size() - 1;
    while (n > 0) {
        if (n - fib[j] >= 0) {
            n -= fib[j];
            ans.push_back(fib[j]);
        }
        j--;        
    }
    sort(ans.begin(), ans.end());
    return ans;
}
