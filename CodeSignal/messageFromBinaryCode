int BinToNum(string s)
{
    int i=0,result=0;
    for (int j=s.length()-1;j>=0;j--)
    {
        result+=(s[j]-'0')*pow(2,i);
        ++i;
    }
    return result;
}
std::string messageFromBinaryCode(std::string code) {
    vector<string> a;
    int current=0;
    string result="";
    while(code.length())
    {
        a.push_back(code.substr(0,8));
        code.erase(0,8);
    }
    for (auto i:a)
    {
        char ch=BinToNum(i);
        result+=ch;
    }
    return result;
}
